{
  "name": "src",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"node ./src/config/serverConfig.js\" \"npm run sass:b\"",
    "serve": "concurrently \"npm run sass:w\" \" npm run debug\"",
    "sass:w": "sass -w src/styles/styles.scss public/css/style.css",
    "sass:b": "sass src/styles/styles.scss public/css/style.css",
    "debug": "SET DEBUG=mybudget-website:* & nodemon ./src/config/serverConfig.js"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "concurrently": "^8.2.0",
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.4",
    "dotenv": "^16.3.1",
    "express": "~4.18.2",
    "hbs": "~4.2.0",
    "http-errors": "~2.0.0",
    "morgan": "~1.10.0",
    "normalize.css": "^8.0.1",
    "sass": "^1.63.6",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}