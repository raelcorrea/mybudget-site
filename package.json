{
  "name": "src",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run sass:b\" \"npm run webpack:b\" \"node ./src/config/server.js\"",
    "serve": "concurrently \"npm run sass:w\" \"npm run webpack:w\" \" npm run debug\"",
    "sass:w": "sass -w src/styles/styles.scss public/css/style.css",
    "sass:b": "sass src/styles/styles.scss public/css/style.css",
    "debug": "SET DEBUG=mybudget-website:* & nodemon ./src/config/server.js",
    "webpack:b": "webpack --mode=production",
    "webpack:w": "webpack --watch --mode=development"
  },
  "dependencies": {
    "@babel/preset-env": "^7.22.9",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "concurrently": "^8.2.0",
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.4",
    "dotenv": "^16.3.1",
    "express": "~4.18.2",
    "hbs": "~4.2.0",
    "http-errors": "~2.0.0",
    "jquery": "^3.7.0",
    "jquery-mask-plugin": "^1.14.16",
    "morgan": "~1.10.0",
    "normalize.css": "^8.0.1",
    "sass": "^1.63.6",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "babel-loader": "^9.1.3",
    "nodemon": "^3.0.1"
  }
}
